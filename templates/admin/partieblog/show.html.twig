{% extends 'base.html.twig' %}

{% block title %}Partieblog
{% endblock %}

{% block body %}
	<h1>Partieblog</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ partieblog.id }}</td>
			</tr>
			<tr>
				<th>Titrepartie</th>
				<td>{{ partieblog.titrepartie }}</td>
			</tr>
			<tr>
				<th>Contenupartie</th>
				<td>{{ partieblog.contenupartie }}</td>
			</tr>
			<tr>
				<th>Blog correspondant :
				</th>
				<td>{{ partieblog.blog.titre }}</td>
			</tr>
		</tbody>
	</table>
	<div class="row gap-3 text-center">
		<a href="{{ path('app_partieblog_index') }}">Liste des parties de blogs</a>

		<a href="{{ path('app_partieblog_edit', {'id': partieblog.id}) }}">Modifier cette partie de blog</a>
		{{ include('admin/partieblog/_delete_form.html.twig') }}
	</div>
{% endblock %}
